<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pagetwonvidia">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/logoutButton"
        android:layout_width="176dp"
        android:layout_height="55dp"
        android:layout_marginTop="450dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/shappingtwo"
        android:backgroundTint="@color/button_background_color_6"
        android:drawableRight="@drawable/ic_baseline_double_arrow_24_1"
        android:drawablePadding="1dp"
        android:fontFamily="@font/geforce_bold"
        android:paddingLeft="25dp"
        android:paddingRight="10dp"
        android:text="lOG OUT"
        android:textColor="@color/button_background_color_5"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nvidiaPageTwo"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="198dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/nvidialogologologo" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="320dp"
        android:layout_height="46dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/geforce_bold"
        android:gravity="left"
        android:shadowColor="@color/text_shadow_white"
        android:shadowDx="5"
        android:shadowDy="1"
        android:shadowRadius="10"
        android:textColor="#55FF00"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/nvidiaPageTwo"
        android:layout_width="320dp"
        android:layout_height="51dp"
        android:fontFamily="@font/geforce_bold"
        android:gravity="left"
        android:padding="1dp"
        android:shadowColor="@color/text_shadow_white"
        android:shadowDx="5"
        android:shadowDy="1"
        android:shadowRadius="10"
        android:text="TO NVIDIA"
        android:textColor="#55FF00"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>